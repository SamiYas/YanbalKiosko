<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.2//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yanbal.kiosko.dao.FormatoArchivoDao">
<!--     <cache />   -->
    <select id="listarTodoPorCorrelativo" resultType="com.yanbal.kiosko.entities.FormatoArchivo" statementType="CALLABLE">
		{CALL KSK_KIOSKO.USP_FORMATO_ARCHIVO_POR_PAIS(#{correlativoFormatoArchivo, mode=IN, jdbcType=BIGINT})}
    </select>
    <select id="obtenerPorCorrelativo" parameterType="map" resultType="com.yanbal.kiosko.entities.FormatoArchivo" statementType="CALLABLE">
		{CALL KSK_KIOSKO.USP_FORMATO_ARCHIVO_SEL_PK(#{correlativoFormatoArchivo, mode=IN, jdbcType=BIGINT})}
    </select>
    <update id="modificar" parameterType="com.yanbal.kiosko.entities.FormatoArchivo" statementType="CALLABLE">
		{CALL KSK_KIOSKO.USP_FORMATO_ARCHIVO_UPD(#{correlativoFormatoArchivo, mode=IN, jdbcType=BIGINT},
										  #{correlativoRol, mode=IN, jdbcType=BIGINT},
										  #{extension,javaType=String,jdbcType=VARCHAR,mode=IN},
										  #{descripcion,javaType=String,jdbcType=LONGVARCHAR,mode=IN},
										  #{descarga,javaType=Double,jdbcType=NUMERIC,mode=IN},
										  #{carga,javaType=Double,jdbcType=NUMERIC,mode=IN},
										  #{usuarioModifica,javaType=String,jdbcType=VARCHAR,mode=IN})}
    </update>   
	
    <select id="listarExtensionTamanho" parameterType="map" resultType="com.yanbal.kiosko.entities.FormatoArchivo" statementType="CALLABLE">
		{CALL KSK_KIOSKO.USP_FORMATO_ARCHIVO_EXT_DESC_POR_PAIS(#{correlativoPais, mode=IN, jdbcType=BIGINT})}	
    </select>
</mapper>