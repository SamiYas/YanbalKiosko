<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.2//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yanbal.kiosko.dao.PaisDao">
    <!-- <cache /> -->  
    <select id="listarTodo" resultType="com.yanbal.kiosko.entities.Pais" statementType="CALLABLE">
		{CALL KSK_KIOSKO.USP_PAIS_SEL_TODOS()}
    </select> 
    <select id="obtenerPorCorrelativo" parameterType="map" resultType="com.yanbal.kiosko.entities.Pais" statementType="CALLABLE">
		{CALL KSK_KIOSKO.USP_PAIS_SEL_PK(#{correlativo, mode=IN, jdbcType=BIGINT})}
    </select>
    <update id="insertarOModificarPorCodigoPais" parameterType="com.yanbal.kiosko.entities.Pais" statementType="CALLABLE">
		{CALL KSK_KIOSKO.USP_INSERT_OR_UPDATE_PAIS(#{codigoPais,javaType=String,jdbcType=VARCHAR,mode=IN},
									  			   #{nombre,javaType=String,jdbcType=VARCHAR,mode=IN})}
    </update>
    <update id="inhabilitarPaises" statementType="CALLABLE">
		{CALL KSK_KIOSKO.USP_INHABILITAR_PAISES()}
    </update>
    <update id="generarFormatoArchivoPaises" statementType="CALLABLE">
		{CALL KSK_KIOSKO.USP_GENERAR_FORMATO_ARCHIVO_PAISES()}
    </update>    
</mapper>